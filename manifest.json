{
    "name": "Word explainer",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "Explains words",
    "icons": {
        "16": "icons/icon.png",
        "32": "icons/icon.png",
        "48": "icons/icon.png",
        "128": "icons/icon.png"
    },
    "permissions": ["sidePanel", "contextMenus"],
    "background": {
        "service_worker": "/scripts/background.js"
    },
    "side_panel": {
        "default_path": "/sidepanel/sidepanel.html"
      }  
}